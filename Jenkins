pipeline {
    agent any
    stages {
        stage("run") {
             steps {
                script {
                    userVar = null
                    passVar = null
withCredentials([conjurSecretUsername(credentialsId: 'MultibranchPipelinewithgit-re-engineering-sub-child-job-credential-1', passwordVariable: 'CONJUR_SECRET', usernameVariable: 'USERNAME')]) {
                                 userVar = CONJUR_SECRET
                                 passVar = USERNAME
                    }
                    echo "Username: ${userVar}"
                    echo "Password: ${passVar}"

                }
            }
        }
    }
}
